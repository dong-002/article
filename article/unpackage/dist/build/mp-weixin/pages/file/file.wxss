.file{padding:30rpx;display:flex;flex-wrap:wrap}.file .box{width:200rpx;height:200rpx;background-color:#eee;margin-right:15rpx;margin-bottom:15rpx;position:relative}.file .box image{width:100%;height:100%}.file .box .close{position:absolute;right:0;top:0;width:30rpx;height:30rpx;background:rgba(0,0,0,.7);color:#fff;padding:0 0 10rpx 10rpx;border-radius:10rpx}.file .add{font-size:80rpx;display:flex;align-items:center;justify-content:center}